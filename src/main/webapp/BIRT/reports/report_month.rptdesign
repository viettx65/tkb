            <xml-property name="queryText"><![CDATA[SELECT 
    a.staff_id, a.staff_name, a.thematic, SUM(a.lesson) total
FROM
    (SELECT 
        s.staff_id,
            st.staff_name,
            s.thematic,
            s.lesson * s.coefficient lesson
    FROM
        sch_schedule s
    LEFT JOIN staff st ON s.staff_id = st.staff_code
    WHERE
        DATE(s.d_from_date) >= DATE(concat(?))
            AND DATE(s.d_from_date) <= DATE(LAST_DAY(DATE(?)))) a
GROUP BY a.staff_id , a.staff_name , a.thematic]]></xml-property>
